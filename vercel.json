{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/scrape",
      "dest": "/api/scrape.py"
    },
    {
      "src": "/api/vet",
      "dest": "/api/vet.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
